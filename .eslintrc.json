{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": "off",
        "no-console": "off",
        "defaultAssignment": "off",
        "no-unneeded-ternary": "off",
        "consistent-return": "off",
        "no-else-return": "off",
        "comma-dangle": "off",
        "implicit-arrow-linebreak": "off",
        "function-paren-newline": "off"
    }
}